{
  "name": "upsc-master-backend",
  "version": "1.0.0",
  "description": "UPSC Master Backend - AWS Serverless",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove:dev": "serverless remove --stage dev",
    "remove:prod": "serverless remove --stage prod",
    "logs": "serverless logs -f",
    "invoke": "serverless invoke -f",
    "offline": "serverless offline",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "csv-parser": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.126",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.9.0",
    "serverless": "^3.38.0",
    "serverless-offline": "^13.3.0",
    "serverless-webpack": "^5.13.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "ts-loader": "^9.5.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "keywords": ["aws", "lambda", "serverless", "upsc", "education", "api"],
  "author": "UPSC Master Team",
  "license": "MIT"
}
